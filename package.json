    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "howler": "^2.2.4",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.294.0",
    "next": "14.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.6",
    "@types/canvas-confetti": "^1.6.4",
