  "homepage": "https://github.com/hanlien/collect-kid-pwa#readme",
  "keywords": [
    "pwa",
    "ai",
    "nature",
    "education",
    "kids",
    "species-recognition",
    "camera",
    "gamification"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:seed": "psql $DATABASE_URL -f supabase.sql",
    "version:patch": "npm version patch --no-git-tag-version && npm run changelog:reminder",
    "version:minor": "npm version minor --no-git-tag-version && npm run changelog:reminder",
    "version:major": "npm version major --no-git-tag-version && npm run changelog:reminder",
    "changelog:reminder": "echo 'ðŸ“ Don\\'t forget to update CHANGELOG.md with release notes!'",
    "release:check": "npm run build && npm run type-check && npm run lint && npm run format:check",
    "release:staging": "npm run release:check && echo 'ðŸš€ Ready for staging deployment'",
    "release:production": "npm run release:check && echo 'ðŸš€ Ready for production deployment'",
    "prerelease": "npm run release:check",
    "release": "git push origin main --tags",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "reset": "npm run clean && npm install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
